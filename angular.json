{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ehelpdesk": {
      "root": "",
      "sourceRoot": "src/main/development",
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "inlineTemplate": false,
          "skipTests": false,
          "prefix": "eh",
          "styleExt": "css"
        },
        "@schematics/angular:directive": {
          "skipTests": false,
          "prefix": "eh"
        },
        "@schematics/angular:guard": {
          "skipTests": false
        },
        "@schematics/angular:pipe": {
          "skipTests": false
        },
        "@schematics/angular:service": {
          "skipTests": false
        }
      },
      "prefix": "eh",
      "architect": {}
    }
  },
  "defaultProject": "ehelpdesk",
  "cli": {
    "packageManager": "yarn"
  }
}
